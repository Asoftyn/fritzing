<?xml version="1.0" encoding="UTF-8"?>
<module fritzingVersion="0.1.1201" moduleId="$uuid">
    <version>1.0</version>
    <title>Basic Resistor</title>
    <label>R</label>
    <date>$date</date>
    <author>Brendan Howell</author>
    <tags>
        <tag>Resistor</tag>
        <tag>basic resistor</tag>
        <tag>fritzing core</tag>
    </tags>
    <properties>
        <property name="Resistance">$resistance Ohms</property>
        <property name="family">Resistor</property>
        <property name="Rated Power">0.25 watt</property>
    </properties>
    <taxonomy>discreteParts.resistor.$resistance</taxonomy>
    <description>A generic resistor (0.25W)</description>
    <views>
        <iconView>
            <layers image="icon/basic-resistor_icon_${resistance}.svg">
                <layer layerId="icon"/>
            </layers>
        </iconView>
        <breadboardView>
            <layers image="breadboard/basic-resistor_breadboard_${resistance}.svg">
                <layer layerId="breadboard"/>
            </layers>
        </breadboardView>
        <schematicView>
            <layers image="schematic/resistor.svg">
                <layer layerId="schematic"/>
            </layers>
        </schematicView>
        <pcbView>
            <layers image="pcb/resistor-025W.svg">
				<layer layerId="copper0" />
				<layer layerId="keepout" />
				<layer layerId="soldermask" />
				<layer layerId="silkscreen" />
				<layer layerId="outline" />
            </layers>
        </pcbView>
    </views>
    <connectors>
        <connector id="connector0" type="male" name="Pin 0">
            <description>Pin0</description>
            <views>
                <breadboardView>
                    <p layer="breadboard" svgId="connector0pin"></p>
                </breadboardView>
                <schematicView>
                    <p layer="schematic" svgId="connector0pin" terminalId="connector0terminal"></p>
                </schematicView>
                <pcbView>
                    <p layer="copper0" svgId="connector0pad"></p>
                </pcbView>
            </views>
        </connector>
        <connector id="connector1" type="male" name="Pin 1">
            <description>Pin 1</description>
            <views>
                <breadboardView>
                    <p layer="breadboard" svgId="connector1pin"></p>
                </breadboardView>
                <schematicView>
                    <p layer="schematic" svgId="connector1pin" terminalId="connector1terminal"></p>
                </schematicView>
                <pcbView>
                    <p layer="copper0" svgId="connector1pad"></p>
                </pcbView>
            </views>
        </connector>
    </connectors>
</module>
