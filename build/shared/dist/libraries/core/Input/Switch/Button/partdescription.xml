<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<part>
	<version>1.0</version>
	<species>PushButton</species>
	<genus>Sensor</genus>
	<title>Push Button</title>
	<description>A general purpose push button</description>
	<icons>
		<icon size="small" source="iconSmall.gif"/>
		<icon size="large" source="iconLarge.gif"/>
	</icons>
	<label>B</label>
	<reference>http://fritzing.org/reference/parts/core/button</reference>
	<defaultUnits>tenths</defaultUnits> <!-- pixels, inches, tenths=inches/10, himetric=cm/1000 -->
	<gridOffset units="himetric" x="-50" y="-50"/>
	<bounds height="635" units="himetric" width="635"/> <!-- 25x25 pixels -->
	<connectors units="tenths">
		<connector id="1" name="A" type="male">
			<point x="0" y="0"/>
		</connector>
		<connector id="2" name="B" type="male">
			<point x="3" y="0"/>
		</connector>
		<connector id="3" name="A" type="male">
			<point x="0" y="2"/>
		</connector>
		<connector id="4" name="B" type="male">
			<point x="3" y="2"/>
		</connector>
	</connectors>
	<nets>
		<trackLayout visible="false"/>
		<net>
			<connector id="1"/>
			<connector id="3"/>
		</net>
		<net>
			<connector id="2"/>
			<connector id="4"/>
		</net>
	</nets>
	<layers>
		<layer type="image">
				<image source="part.png" type="bitmap" zoom="1"/>
		</layer>
	</layers>
	<footprints>
		<footprint default="true" source="fritzing.lbr/Button" type="eagle"/>
	</footprints>
</part>
