<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<part>
	<version>1.0</version>
	<species>PushButton</species>
	<genus>Input.Switch</genus>
	<title>Push Button</title>
	<description>A general purpose push button</description>
	<icons>
		<icon size="small" source="iconSmall.gif"/>
		<icon size="large" source="iconLarge.gif"/>
	</icons>
	<label>B</label>
	<reference>http://fritzing.org/reference/parts/core/button</reference>
	<authors>
		<author url="http://fritzing.org/author/dirk">Dirk van Oosterbosch</author>
	</authors>
	<defaultUnits>tenths</defaultUnits> <!-- pixels, inches, tenths=inches/10, himetric=cm/1000 -->
	<gridOffset x="64" y="-64" units="himetric"/> <!-- 10x-10 400% view pixels -->
	<bounds width="635" height="635" units="himetric"/> <!-- 100x100 400% view pixels -->
	<connectors units="tenths">
		<connector id="1" name="A" type="leg">
			<!-- <point x="0" y="254" units="himetric"/> -->
			<point x="0" y="64" units="himetric"/>
			<point x="0" y="0"/>
		</connector>
		<connector id="2" name="B" type="leg">
			<!-- <point x="508" y="254" units="himetric"/> -->
			<point x="508" y="64" units="himetric"/>
			<point x="3" y="0"/>
		</connector>
		<connector id="3" name="A" type="leg">
			<!-- <point x="0" y="508" units="himetric"/> -->
			<point x="0" y="699" units="himetric"/>
			<point x="0" y="3"/>
		</connector>
		<connector id="4" name="B" type="leg">
			<!-- <point x="508" y="508" units="himetric"/> -->
			<point x="508" y="699" units="himetric"/>
			<point x="3" y="3"/>
		</connector>
	</connectors>
