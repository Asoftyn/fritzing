<?xml version="1.0" encoding="UTF-8"?>
<part>
	<version>1.0</version>
	<species>PushButton</species>
	<genus>Sensor</genus>
	<title>Push Button</title>
	<description>A general purpose push button</description>
	<icons>
		<icon size="small" source="iconSmall.gif" />
		<icon size="large" source="iconLarge.gif" />
	</icons>
	<label>B</label>
	<reference>http://fritzing.org/reference/parts/core/button</reference>
	<defaultUnits>tenths</defaultUnits> <!-- pixels, inches, tenths=inches/10, himetric=cm/1000 -->
	<gridOffset x="-50" y="-50" units="himetric"/>
	<bounds width="635" height="635" units="himetric"/> <!-- 25x25 pixels -->
	<connectors units="tenths">
		<connector id="1" name="A" x="0" y="0" type="male" />
		<connector id="2" name="B" x="3" y="0" type="male" />
		<connector id="3" name="A" x="0" y="2" type="male" />
		<connector id="4" name="B" x="3" y="2" type="male" />
	</connectors>
	<nets>
		<trackLayout visible="false" />
		<net><connector id="1"/><connector id="3"/></net>
		<net><connector id="2"/><connector id="4"/></net>
	</nets>
	<layers>
		<layer type="image">
				<image type="bitmap" source="part.png" zoom="1"/>
		</layer>
	</layers>
	<footprints>
		<footprint type="eagle" source="fritzing.lbr/Button" default="true" />
	</footprints>
</part>