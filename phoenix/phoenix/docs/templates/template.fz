<?xml version="1.0" encoding="UTF-8"?>
<module uri="http://fritzing.org/modules/npn-Transistor" moduleId="1234ABDC24">
	<version>1.1</version> 
	<author>Brendan Howell</author>
	<title>Red LED - 5mm</title>
	<date>2008-07-25</date>
	<tags>LED, Red LED, indicator, fritzing core</tags>
	<properties>LED, Red LED, diode 1.8V</properties>
	<taxonomy>discreteParts.LED.5mm.red</taxonomy>
	<description>A generic red LED (~1.8V)</description>
	<views>
	    <image file="LED-red-5mm.svg" composited="false" />
	    <defaultUnits>cmil</defaultUnits>
	    <iconView>
			<layers>
				<layer image="icon.svg" layerId="icon" />
			</layers>
		</iconView>
    	<breadboardView>
			<layers>
				<layer image="breadboard.svg" layerId="breadboard" />
				<layer image="breadboardconnector.svg" layerId="breadboardConnector" />
			</layers>
    	</breadboardView>
    	<schematicView>
			<layers>
				<layer image="schematic.svg" layerId="schematic" />
				<layer image="schematicconnector.svg" layerId="schematicConnector" />
			</layers>
    	</schematicView>
    	<pcbView>
    	    <label></label>
    	    <layers>
    	        <layer layerId="silkscreen" image="silkscreen.svg" />
    	        <layer layerId="copper0" image="copper0.svg" />
	            <layer layerId="soldermask" image="soldermask.svg" />
	            <layer layerId="outline" image="outline.svg" /> 
    	    </layers>
    	</pcbView>
    </views>
	
	<connectors>
    	<connector id="connector0" name="anode" type="male">
    	    <description>anode pin</description>
    	    <views>
        	    <breadboardView>
        	        <pin layer="breadboardConnector" svgId="connector0pin" />  
        	    </breadboardView>
        	    <schematicView>
        	        <pin layer="schematicConnector" svgId="connector0pin" terminalId="connector0terminal" />
        	    </schematicView>
        	    <pcbView>
        	        <pin layer="copper0" svgId="connector0pin" /> <!-- this id refers to a specific path in the SVG file -->
        	    </pcbView>
    	    </views>
    	</connector>
    	<connector id="connector1" name="cathode" type="male">
    	    <description>cathode pin</description>
    	    <views>
        	    <breadboardView>
        	        <pin layer="breadboardConnector" svgId="connector1pin" />  
        	    </breadboardView>
        	    <schematicView>
        	        <pin layer="schematicConnector" svgId="connector1pin" terminalId="connector1terminal"/>
        	    </schematicView>
        	    <pcbView>
        	        <pin layer="copper0" svgId="connector1pin" />
        	    </pcbView>
    	    </views>
    	</connector>
	</connectors>
</module>
