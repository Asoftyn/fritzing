<?xml version='1.0' encoding='UTF-8'?>
<module moduleId="$uuid" >
 <version>1.0</version>
 <author>Fritzing Part-o-matic</author>
 <title>DIP - $pins pins</title>
 <label>IC</label>
 <date>$date</date>
 <tags>
  <tag>DIP</tag>
  <tag>fritzing core</tag>
 </tags>
 <properties>
  <property name="family" >DIP</property>
  <property name="pins">$pins</property>
  <property name="spacing">$spacing</property>
  <property name="chip label">IC</property>
 </properties>
 <taxonomy>part.dip.${pins}pins</taxonomy>
 <description>A DIP stand-in part for whatever you want.  With $pins pins on a 100 mil dual inline pin header footprint spaced $spacing.</description>
 <views>
  <iconView>
   <layers image="icon/dip_icon.svg" >
    <layer layerId="icon" />
   </layers>
  </iconView>
  <breadboardView>
   <layers image="breadboard/DIP_bread_${spacing}_${pins}.svg" >
    <layer layerId="breadboard" />
   </layers>
  </breadboardView>
  <schematicView>
   <layers image="schematic/DIP_schem_${pins}.svg" >
    <layer layerId="schematic" />
   </layers>
  </schematicView>
  <pcbView>
   <layers image="pcb/DIP_pcb_${spacing}_${pins}.svg" >
    <layer layerId="silkscreen" />
    <layer layerId="copper0" />
   </layers>
  </pcbView>
 </views>
 <connectors>
#for $pin in range(int($pins))
  #set $pindex = $pin + 1
  <connector type="male" id="connector$pin" name="leg$pindex" >
   <description>pin $pindex</description>
   <views>
    <breadboardView>
     <p svgId="connector${pin}pin" layer="breadboard" terminalId="connector${pin}terminal" />
    </breadboardView>
    <schematicView>
     <p svgId="connector${pin}pin" layer="schematic" terminalId="connector${pin}terminal" />
    </schematicView>
    <pcbView>
     <p svgId="connector${pin}pin" layer="copper0" />
    </pcbView>
   </views>
  </connector>
#end for
 </connectors>
</module>
