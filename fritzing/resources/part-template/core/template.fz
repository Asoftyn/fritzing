<?xml version="1.0" encoding="UTF-8"?>
<module uri="http://fritzing.org/modules/npn-Transistor" moduleId="1E34ABDC24">
	<version>1.0</version> 
	<author>Fritz Bauer</author>
	<title>Widgitech - Examplotron</title>
	<date>2008-07-25</date>
	<tags>
		<tag>example parts</tag>
	</tags>
	<properties>
		<property name="p1">part template</property>
	</properties>
	<taxonomy>discreteParts.examples.template.base</taxonomy>
	<description>Sample Part Template</description>
	<views>
	    <image file="LED-red-5mm.svg" composited="false" />
	    <defaultUnits>cmil</defaultUnits>
	    <iconView>
			<layers>
				<layer image="icon/icon.svg" layerId="icon" />
			</layers>
		</iconView>
    	<breadboardView>
			<layers>
				<layer image="breadboard/part/breadboard.svg" layerId="breadboard" />
				<layer image="breadboard/connector/breadboardconnector.svg" layerId="breadboardConnector" />
			</layers>
    	</breadboardView>
    	<schematicView>
			<layers>
				<layer image="schematic/part/schematic.svg" layerId="schematic" />
				<layer image="schematic/connector/schematicconnector.svg" layerId="schematicConnector" />
			</layers>
    	</schematicView>
    	<pcbView>
    	    <label></label>
    	    <layers>
    	        <layer layerId="silkscreen" image="pcb/silkscreen/silkscreen.svg" />
    	        <layer layerId="copper0" image="pcb/copper/copper0.svg" />
	            <layer layerId="soldermask" image="pcb/soldermask/soldermask.svg" />
	            <layer layerId="outline" image="pcb/outline/outline.svg" /> 
    	    </layers>
    	</pcbView>
    </views>
	
	<connectors>
    	<connector id="connector0" name="pin0" type="male">
    	    <description>anode pin</description>
    	    <views>
        	    <breadboardView>
        	        <pin layer="breadboardConnector" svgId="connector0pin" />  
        	    </breadboardView>
        	    <schematicView>
        	        <pin layer="schematicConnector" svgId="connector0pin" terminalId="connector0terminal" />
        	    </schematicView>
        	    <pcbView>
        	        <pin layer="copper0" svgId="connector0pin" /> <!-- this id refers to a specific path in the SVG file -->
        	    </pcbView>
    	    </views>
    	</connector>
    	<connector id="connector1" name="pin1" type="male">
    	    <description>cathode pin</description>
    	    <views>
        	    <breadboardView>
        	        <pin layer="breadboardConnector" svgId="connector1pin" />  
        	    </breadboardView>
        	    <schematicView>
        	        <pin layer="schematicConnector" svgId="connector1pin" terminalId="connector1terminal"/>
        	    </schematicView>
        	    <pcbView>
        	        <pin layer="copper0" svgId="connector1pin" />
        	    </pcbView>
    	    </views>
    	</connector>
	</connectors>
</module>
