/* ################### the toolbar #################### */
QToolBar {
	border: 0px;
	/* spacing: 10px; spacing of items in the toolbar */

	background-color: rgb(51,0,17);
	color: rgb(126,54,77);
}
qtoolbutton
#fileToolBar QToolButton,
