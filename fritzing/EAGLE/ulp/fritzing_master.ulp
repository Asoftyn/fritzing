// Creates PCB from a Fritzing2Eagle file
// Paramaters: the Fritzing2Eagle file to start from
// Run from the shell like this: eagle -C "RUN 'fritzing_master.ulp' '<fritzing2eagleFile.scr>'" newSchematicFile.sch

string c,cmd;

cmd += "script '" + argv[1] + "'; \n";
cmd += "run " + argv[2] + "fritzing_autoplace; \n";
cmd += "script fritzing_setup-arduino_shield; \n";
cmd += "script fritzing_menu-placement; \n";

exit(cmd);
