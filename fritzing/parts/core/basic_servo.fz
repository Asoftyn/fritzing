<?xml version="1.0" encoding="UTF-8"?>
<module uri="http://fritzing.org/modules/potentiometer" moduleId="3234DBDC80">
    <version>1.1</version>
    <author>Stefan Hermann</author>
    <title>Basic Servo</title>
    <label>J</label>
    <date>2008-11-04</date>
    <tags>
    	<tag>servo</tag>
    	<tag>fritzing core</tag>
    </tags>
    <properties>
    	<property name="family">Servo</property>
    </properties>
    <taxonomy>discreteParts.servo</taxonomy>
    <description>A generic servo.</description>
    <views>
        <image composited="false" file="basic_poti.svg" />
        <defaultUnits>cmil</defaultUnits>
        <iconView>
            <layers>
                <layer layerId="icon" image="icon/basic_servo.svg"/>
            </layers>
        </iconView>
        <breadboardView>
            <layers>
                <layer layerId="breadboard" image="breadboard/basic_servo.svg"/>
            </layers>
        </breadboardView>
        <schematicView>
            <layers>
                <layer layerId="schematic" image="schematic/basic_poti.svg"/>
            </layers>
        </schematicView>
        <pcbView>
            <layers>
                <layer layerId="silkscreen" image="pcb/jumper3.svg"/>
                <layer layerId="copper0" image="pcb/jumper3.svg"/>
            </layers>
        </pcbView>
    </views>
    <connectors>
        <connector type="male" id="connector0" name="gnd" >
            <description>ground</description>
            <views>
                <breadboardView>
                    <pin layer="breadboard" svgId="connector0pin"/>
                </breadboardView>
                <schematicView>
                    <pin layer="schematic" svgId="connector0pin" terminalId="connector0terminal" />
                </schematicView>
                <pcbView>
                    <pin layer="copper0" svgId="connector0pin" />
                </pcbView>
            </views>
        </connector>
        <connector type="male" id="connector1" name="vcc" >
            <description>vcc</description>
            <views>
                <breadboardView>
                    <pin layer="breadboard" svgId="connector1pin"/>
                </breadboardView>
                <schematicView>
                    <pin layer="schematic" svgId="connector1pin" terminalId="connector1terminal" />
                </schematicView>
                <pcbView>
                    <pin layer="copper0" svgId="connector1pin" />
                </pcbView>
            </views>
        </connector>
        <connector type="male" id="connector2" name="pulse" >
            <description>pulse</description>
            <views>
                <breadboardView>
                    <pin layer="breadboard" svgId="connector2pin"/>
                </breadboardView>
                <schematicView>
                    <pin layer="schematic" svgId="connector2pin" terminalId="connector2terminal" />
                </schematicView>
                <pcbView>
                    <pin layer="copper0" svgId="connector2pin" />
                </pcbView>
            </views>
        </connector>
    </connectors>
</module>
