<?xml version="1.0" encoding="UTF-8"?>
<module fritzingVersion="0.3.13b.12.02.3806" moduleId="a7ec63797771e5b6f370dca4ff17bc81">
    <version>1.0</version>
    <author>Lionel Michel</author>
    <title>3-Axis Accelerometer Breakout</title>
    <label>U</label>
    <date>2009-12-09</date>
    <tags>
        <tag>accelerometer</tag>
        <tag>orientation</tag>
				<tag>sensor</tag>
				<tag>breakout</tag>
    </tags>
    <properties>
        <property name="family">Accelerometers</property>
        <property name="axes">3 (x, y, z)</property>
				<property name="chip">ADXL</property>
    </properties>
    <description>Accelerometer for sensing three-dimensional orientation/acceleration on a breakout board. Compatible to the ADXL3xx series of sensor chips.</description>
    <views>
        <breadboardView>
            <layers image="breadboard/accelerometer-adxl.svg">
                <layer layerId="breadboard"/>
            </layers>
        </breadboardView>
        <schematicView>
            <layers image="schematic/accelerometer-adxl.svg">
                <layer layerId="schematic"/>
            </layers>
        </schematicView>
        <pcbView>
            <layers image="pcb/jumper_6_100mil_pcb.svg">
                <layer layerId="copper0"/>
                <layer layerId="silkscreen"/>
            </layers>
        </pcbView>
        <iconView>
            <layers image="icon/accelerometer-adxl.svg">
                <layer layerId="icon"/>
            </layers>
        </iconView>
    </views>
    <connectors>
        <connector id="connector0" type="male" name="ST">
            <description>Self-Test</description>
            <views>
                <breadboardView>
                    <p layer="breadboard" svgId="connector0pin"/>
                </breadboardView>
                <schematicView>
                    <p layer="schematic" svgId="connector0pin" terminalId="connector0terminal"/>
                </schematicView>
                <pcbView>
                    <p layer="copper0" svgId="connector0pin"/>
                </pcbView>
            </views>
        </connector>
        <connector id="connector1" type="male" name="Z">
            <description>Z-axis</description>
            <views>
                <breadboardView>
                    <p layer="breadboard" svgId="connector1pin"/>
                </breadboardView>
                <schematicView>
                    <p layer="schematic" svgId="connector1pin" terminalId="connector1terminal"/>
                </schematicView>
                <pcbView>
                    <p layer="copper0" svgId="connector1pin"/>
                </pcbView>
            </views>
        </connector>
        <connector id="connector2" type="male" name="Y">
            <description>Y-axis</description>
            <views>
                <breadboardView>
                    <p layer="breadboard" svgId="connector2pin"/>
                </breadboardView>
                <schematicView>
                    <p layer="schematic" svgId="connector2pin" terminalId="connector2terminal"/>
                </schematicView>
                <pcbView>
                    <p layer="copper0" svgId="connector2pin"/>
                </pcbView>
            </views>
        </connector>
        <connector id="connector3" type="male" name="X">
            <description>X-axis</description>
            <views>
                <breadboardView>
                    <p layer="breadboard" svgId="connector3pin"/>
                </breadboardView>
                <schematicView>
                    <p layer="schematic" svgId="connector3pin" terminalId="connector3terminal"/>
                </schematicView>
                <pcbView>
                    <p layer="copper0" svgId="connector3pin"/>
                </pcbView>
            </views>
        </connector>
        <connector id="connector4" type="male" name="GND">
            <description>Ground</description>
            <views>
                <breadboardView>
                    <p layer="breadboard" svgId="connector4pin"/>
                </breadboardView>
                <schematicView>
                    <p layer="schematic" svgId="connector4pin" terminalId="connector4terminal"/>
                </schematicView>
                <pcbView>
                    <p layer="copper0" svgId="connector4pin"/>
                </pcbView>
            </views>
        </connector>
        <connector id="connector5" type="male" name="VDD">
            <description>Power</description>
            <views>
                <breadboardView>
                    <p layer="breadboard" svgId="connector5pin"/>
                </breadboardView>
                <schematicView>
                    <p layer="schematic" svgId="connector5pin" terminalId="connector5terminal"/>
                </schematicView>
                <pcbView>
                    <p layer="copper0" svgId="connector5pin"/>
                </pcbView>
            </views>
        </connector>
    </connectors>
</module>
